{
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "printWidth": 100,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "[typescriptreact]": {
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "plugins": ["prettier-plugin-tailwindcss"]
}
